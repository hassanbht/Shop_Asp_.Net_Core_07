@model List<string>

      <a class="btn btn-block btn-dark"
      asp-controller="Home"
      asp-action="Index"
      asp-route-category=""
      asp-route-pageNumber="1">Home</a>
@foreach (var item in Model)
{
    <a class="btn btn-block  @(item == ViewBag.Category ? "btn-success": "btn-dark")"
      asp-controller="Home"
      asp-action="Index"
      asp-route-category="@item"
      asp-route-pageNumber="1">@item</a>
}